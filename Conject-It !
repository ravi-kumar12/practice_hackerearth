t=int(input())
while t!=0:
    n=int(input())
    while True:
        
        if n==1:
            print("YES")
            break
        elif n%2==0:
            n=n/2
            a=n
            n=int(n)
            if a!= n:
                print("NO")
                break
        else:
            n=3*n+1
    t-=1
